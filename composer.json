{
    "name": "parasek/concrete5",
    "description": "A fully featured concrete5 solution comprising framework skeleton, Docker server and basic theme.",
    "type": "project",
    "homepage": "https://github.com/parasek/concrete5",
    "license": "MIT",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Application\\": "./public/application/src"
        },
        "files": [
            "public/application/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\": "./tests"
        }
    },
    "require": {
        "composer/installers": "^1.3",
        "concrete5/core": "^9.0.1",
        "concrete5/dependency-patches": "^1.4.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3|^8.0",
        "mockery/mockery": "^0.9.9|^1.2"
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "mlocati/composer-patcher": true
        }
    },
    "extra": {
        "allow-subpatches": [
            "concrete5/dependency-patches"
        ],
        "branch-alias": {
            "dev-8.x": "8.x-dev"
        },
        "installer-paths": {
            "public/concrete": [
                "type:concrete5-core"
            ],
            "public/application/themes/{$name}": [
                "type:concrete5-theme"
            ],
            "public/packages/{$name}": [
                "type:concrete5-package"
            ],
            "public/application/blocks/{$name}": [
                "type:concrete5-block"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
